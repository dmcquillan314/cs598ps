function l10_39( )
    figure;
    x = [ 1 6; 1 10; 4 11; 5 2; 7 6; 10 4 ];
    z = [ 1; 1; 1; -1; -1; -1; ];

    [w, w_0] = quadprog_svm(x,z);
    
    Xax = 0:20; 
    Yax=-(w_0+Xax*w(1))/w(2);
    
    
    scatter(x(:,1),x(:,2));
     axis([ 0 12 0 12 ]);
    hold on;
    plot(Xax,Yax);
    hold off;
    
    figure;
    MU1 = [2 2];
    SIGMA1 = [0.75 0; 0 0.7];
    MU2 = [-3 -2.5];
    SIGMA2 = [0.75 0; 0 0.7];

    D1 = probDist(MU1, SIGMA1, 20);
    y1 = ones(20, 1);
    D2 = probDist(MU2, SIGMA2, 20);
    y2 = -1 * ones(20, 1);
    
    X = [D1; D2];
    y = [y1; y2];
    
    
    
    scatter(D1(:,1),D1(:,2))
    
    hold on;
    scatter(D2(:,1),D2(:,2))
    hold off;
    
    axis([ -4 4 -4 4 ]);
end

