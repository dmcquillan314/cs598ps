function [ output_args ] = l5_39( input_args )

    faces = l5_faces;
    ;
    N = 11;
    MeanFace = mean(FaceData);
    ZeroMeanFaces = FaceData - repmat(MeanFace, [600 1]);
    [Features, Vals, Weights] = svd(ZeroMeanFaces');
    LimF = Features(:,1:10);
    LimW = Vals(1:10, :) * Weights()';

end

function [F] = reconstruct(faces, v, f)
    N = size(faces.X, 2);

    X = faces.X';
    avg_face = mean(X);
    zm_faces = X - repmat(avg_face, [N 1]);
    
    [F, V, W] = svd(zm_faces');
    
    P = V(1:10,:) * W';
    recon = F(:, 1:v) * P(:, f) + avg_face;
    
    F = recon
end
